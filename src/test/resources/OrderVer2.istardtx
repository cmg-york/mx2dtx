<?xml version="1.0" encoding="UTF-8"?>
<iStarDT xmlns="https://example.org/istar-dt-x"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://example.org/istar-dt-x ../xsd/istar-rl-schema_v4.xsd">


<!--
*** H E A D E R ***
-->

<header title = ""
    author = ""
    source = ""
    lastUpdated = "Test">
</header>


<!--
*** O P T I O N S ***
-->
<options continuous = "false"
    infeasibleActionPenalty = "100">
</options>


<!--
*** A C T O R S ***
-->
<actors>
<actor name="default" description="">


<!--
*** G O A L S ***
-->
<goals>
   <goal name = "orderMaterial" description = "Order Material" root = "true" terminal = "false" episodeLength = "1">
      <refinement type = "OR">
         <childTask ref = "orderFromSupplierA"/>
         <childTask ref = "orderFromSupplierB"/>
      </refinement>
   </goal>
</goals>


<!--
*** T A S K S ***
-->
<tasks>
   <task name = "orderFromSupplierA" description = "Order From Supplier A">
      <effectGroup>
         <effect name = "deliveredInTimeA_Eff" description = "Delivered In Time A" satisfying = "true" probability = "0.75">
            <turnsTrue>aTruePredicate</turnsTrue>
            <turnsTrue>anotherTruepredicate</turnsTrue>
            <turnsFalse>falsePredicate</turnsFalse>
            <turnsFalse>anotherfalsePredicate</turnsFalse>
            <set><variableID>someVar</variableID><numConst>12</numConst></set>
         </effect>
         <effect name = "neverDeliveredA_Eff" description = "neverDeliveredA" satisfying = "false" probability = "0.05">
            <turnsTrue>signalONS</turnsTrue>
            <turnsFalse>hvacOn</turnsFalse>
            <turnsFalse>signalOffS</turnsFalse>
         </effect>
         <effect name = "deliveredLateA_Eff" description = "deliveredLateA" satisfying = "true" probability = "0.2">
            <turnsTrue>deliveredLateA</turnsTrue>
         </effect>
      </effectGroup>
      <pre>
         <predicateID>deliveredLateB</predicateID>
      </pre>
   </task>
   <task name = "orderFromSupplierB" description = "Order From Supplier B">
      <effectGroup>
         <effect name = "deliveredInTimeB_Eff" description = "deliveredInTimeB" satisfying = "true" probability = "0.5">
            <turnsTrue>deliveredInTimeB</turnsTrue>
         </effect>
         <effect name = "neverDeliveredB_Eff" description = "neverDeliveredB" satisfying = "false" probability = "0.15">
            <turnsTrue>neverDeliveredB</turnsTrue>
         </effect>
         <effect name = "deliveredLateB_Eff" description = "deliveredLateB" satisfying = "true" probability = "0.35">
            <turnsTrue>deliveredLateB</turnsTrue>
         </effect>
      </effectGroup>
      <pre>
         <and><or><and><predicateID>hvacOn</predicateID><not><predicateID>signalOffS</predicateID></not></and><predicateID>signalONS</predicateID></or><or><predicateID>deliveredLateA</predicateID><predicateID>deliveredInTimeB</predicateID></or></and>
      </pre>
   </task>
</tasks>


<!--
*** Q U A L I T I E S ***
-->
<qualities>
   <quality name = "cost" description = "Cost" root = "false">
      <add>
         <multiply><numConst>0.5</numConst><predicateID>deliveredInTimeA</predicateID></multiply>
         <multiply><numConst>0.5</numConst><predicateID>deliveredLateA</predicateID></multiply>
         <multiply><numConst>0.5</numConst><predicateID>neverDeliveredA</predicateID></multiply>
         <multiply><numConst>1.0</numConst><predicateID>deliveredInTimeB</predicateID></multiply>
         <multiply><numConst>1.0</numConst><predicateID>deliveredLateB</predicateID></multiply>
         <multiply><numConst>1.0</numConst><predicateID>neverDeliveredB</predicateID></multiply>
      </add>
   </quality>
   <quality name = "happyCustomer" description = "Happy Customer" root = "false">
      <add>
         <multiply><numConst>1.0</numConst><predicateID>deliveredInTimeA</predicateID></multiply>
         <multiply><numConst>0.7</numConst><predicateID>deliveredLateA</predicateID></multiply>
         <multiply><numConst>1.0</numConst><predicateID>deliveredInTimeB</predicateID></multiply>
         <multiply><numConst>0.7</numConst><predicateID>deliveredLateB</predicateID></multiply>
      </add>
   </quality>
   <quality name = "overallQuality" description = "Overall Quality" root = "true">
      <add>
         <multiply><numConst>0.7</numConst><qualID>cost</qualID></multiply>
         <multiply><numConst>0.3</numConst><qualID>happyCustomer</qualID></multiply>
      </add>
   </quality>
   <quality name = "lonelyQuality" description = "lonelyQuality" root = "false">
    <add>
         <multiply><numConst>0.5</numConst><predicateID>deliveredLateA</predicateID></multiply>
         <multiply><numConst>1.0</numConst><predicateID>neverDeliveredB</predicateID></multiply>
      </add>
   </quality>
</qualities>


<!--
*** CO N D I T I O N   B O X E S ***
-->
<condBoxes>
<condBox name = "deliveredInTimeXANDD"><or><and><predicateID>hvacOn</predicateID><not><predicateID>signalOffS</predicateID></not></and><predicateID>signalONS</predicateID></or></condBox>
<condBox name = "deliveredLateAORDeliveredInTimeB"><or><predicateID>deliveredLateA</predicateID><predicateID>deliveredInTimeB</predicateID></or></condBox>
<condBox name = "deliveredLateB"><predicateID>deliveredLateB</predicateID></condBox>
<condBox name = "lonely"><or><previous><predicateID>deliveredLateA</predicateID></previous><predicateID>neverDeliveredB</predicateID></or></condBox>
</condBoxes>


<!--
*** I N I T I A L I Z AT I O N S ***
-->
<initializations>
      <initialization element = "deliveredLateA">true</initialization>
      <initialization element = "happyCustomer">10</initialization>
      <initialization element = "cost">0</initialization>
      <initialization element = "someVar">100</initialization>
      <initialization element = "overallQuality">0</initialization>
</initializations>


<!--
*** C R O S S   R U N S  ***
-->
<crossRuns>
      <crossRun><qualID>cost</qualID></crossRun>
      <crossRun><qualID>happyCustomer</qualID></crossRun>
      <crossRun><variableID>someVar</variableID></crossRun>
      <crossRun><predicateID>aTruePredicate</predicateID></crossRun>
      <crossRun><predicateID>neverDeliveredB</predicateID></crossRun>
</crossRuns>


<!--
*** E X P O R T S ***
-->
<exportedSet>
      <export continuous = "false"><predicateID>deliveredInTimeB</predicateID></export>
      <export continuous = "false"><goalID>orderMaterial</goalID></export>
      <export continuous = "false"><taskID>orderFromSupplierA</taskID></export>
      <export continuous = "true" minVal = "0" maxVal = "100"><qualID>cost</qualID></export>
      <export continuous = "true" minVal = "0" maxVal = "200"><variableID>someVar</variableID></export>
      <export continuous = "true" minVal = "0" maxVal = "1"><qualID>overallQuality</qualID></export>
</exportedSet>


<!--
*** P R E D I C A T E S ***
-->
<predicates>
   <predicate description = "">neverDeliveredB</predicate>
   <predicate description = "">deliveredInTimeB</predicate>
   <predicate description = "">hvacOn</predicate>
   <predicate description = "">falsePredicate</predicate>
   <predicate description = "">deliveredLateB</predicate>
   <predicate description = "">aTruePredicate</predicate>
   <predicate description = "">signalONS</predicate>
   <predicate description = "">anotherfalsePredicate</predicate>
   <predicate description = "">deliveredLateA</predicate>
   <predicate description = "">anotherTruepredicate</predicate>
   <predicate description = "">signalOffS</predicate>
</predicates>


<!--
*** V A R I A B L E S ***
-->
<variables>
   <variable description = "">someVar</variable>
</variables>

</actor>
</actors>
</iStarDT>